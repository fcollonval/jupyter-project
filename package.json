{
  "name": "jupyter-project",
  "version": "1.0.0",
  "description": "An JupyterLab extension to handle project and files template.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "template"
  ],
  "homepage": "https://github.com/fcollonval/jupyter-project",
  "bugs": {
    "url": "https://github.com/fcollonval/jupyter-project/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Frederic Collonval",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcollonval/jupyter-project.git"
  },
  "scripts": {
    "build": "jlpm run build:lib",
    "build:labextension": "cd jupyter_project && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:all": "jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyter_project/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "lint": "eslint . --ext .ts,.tsx --fix && jlpm run embedme README.md",
    "lint:check": "eslint . --ext .ts,.tsx && jlpm run embedme --verify README.md",
    "prepare": "jlpm run clean && jlpm run build",
    "test": "jest --coverage",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.2.0",
    "@jupyterlab/apputils": "^1.2.0",
    "@jupyterlab/coreutils": "^3.2.0",
    "@jupyterlab/filebrowser": "^1.2.0",
    "@jupyterlab/git": ">=0.10 <0.20.0",
    "@jupyterlab/launcher": "^1.2.0",
    "@jupyterlab/mainmenu": "^1.2.0",
    "@jupyterlab/services": "^4.2.0",
    "@jupyterlab/statusbar": "^1.2.0",
    "@jupyterlab/ui-components": "^1.2.0",
    "@material-ui/core": "^4.9.13",
    "@phosphor/signaling": "^1.3.0",
    "@phosphor/widgets": "^1.9.0",
    "ajv": "^6.12.2",
    "jupyterlab_conda": "^1.3.1",
    "jupyterlab_toastify": "^4.0.0",
    "uniforms": "^2.6.7",
    "uniforms-bridge-json-schema": "^2.6.7",
    "uniforms-material": "^2.6.7",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@jupyterlab/testutils": "^1.2.0",
    "@types/jest": "^25.2.3",
    "@types/react": "~16.8.13",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "embedme": "^1.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.0.0",
    "mkdirp": "^1.0.3",
    "prettier": "^1.16.4",
    "react": "~16.8.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^25.0.0",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip",
          "conda"
        ],
        "base": {
          "name": "jupyter-project"
        }
      }
    },
    "extension": true
  },
  "resolutions": {
    "@types/react": "~16.8.4"
  }
}
